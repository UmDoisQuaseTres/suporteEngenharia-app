<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<!-- Verifique se a versão do runtime está correta para seu projeto -->
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
		<!-- Ou se for .NET 6+, pode ser diferente ou não ter essa seção -->
	</startup>
	<appSettings>
		<!-- !! IMPORTANTE: Coloque a URL CORRETA da sua API aqui !! -->
		<add key="ApiBaseUrl" value="http://192.168.15.49:5000/" />
		<!-- Timeout em segundos -->
		<add key="ApiTimeoutSeconds" value="2" />
		<!-- Se sua API precisar de um token, adicione aqui -->
		<!-- <add key="ApiToken" value="SEU_TOKEN_SECRETO_AQUI" /> -->
	</appSettings>
</configuration>